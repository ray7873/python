temperature = int(input("Enter the temperature: "))

if temperature > 30:
    print("It's warm")
    print("Drink more water")
    print("Stay hydrated")
elif temperature < 25:
    print("It's normal")
    print("Get going")
else:
    print("Take winter gear")
    print("It's cold")
